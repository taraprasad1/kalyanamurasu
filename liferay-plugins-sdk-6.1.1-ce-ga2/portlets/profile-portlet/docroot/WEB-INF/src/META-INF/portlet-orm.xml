<?xml version="1.0"?>

<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.0" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_1_0.xsd">
	<persistence-unit-metadata>
		<xml-mapping-metadata-complete/>
		<persistence-unit-defaults>
			<access>PROPERTY</access>
		</persistence-unit-defaults>
	</persistence-unit-metadata>
	<mapped-superclass class="com.vijayan.profile.model.impl.KeyModelImpl">
		<attributes>
			<id name="keyId" />
			<basic name="name" />
			<transient name="modelAttributes" />
			<transient name="primaryKey" />
			<transient name="primaryKeyObj" />
		</attributes>
	</mapped-superclass>
	<mapped-superclass class="com.vijayan.profile.model.impl.KeyValueModelImpl">
		<attributes>
			<id name="valueId" />
			<basic name="keyId" />
			<basic name="name" />
			<transient name="modelAttributes" />
			<transient name="primaryKey" />
			<transient name="primaryKeyObj" />
		</attributes>
	</mapped-superclass>
	<mapped-superclass class="com.vijayan.profile.model.impl.ProfileModelImpl">
		<attributes>
			<id name="profileId" />
			<basic name="profileCode" />
			<basic name="createdForMy" />
			<basic name="name" />
			<basic name="gender" />
			<basic name="birthDateWithTime">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="rasi" />
			<basic name="star" />
			<basic name="dosam" />
			<basic name="religion" />
			<basic name="caste" />
			<basic name="subCaste" />
			<basic name="height" />
			<basic name="weight" />
			<basic name="complexion" />
			<basic name="motherTongue" />
			<basic name="languageKnown" />
			<basic name="maritalStatus" />
			<basic name="children" />
			<basic name="country" />
			<basic name="state">
				<column name="STATE_" />
			</basic>
			<basic name="city" />
			<basic name="address" />
			<basic name="pinCode" />
			<basic name="phone" />
			<basic name="mobile" />
			<basic name="emailAddress" />
			<basic name="education" />
			<basic name="profession" />
			<basic name="currency">
				<column name="CURRENCY_" />
			</basic>
			<basic name="annualIncome" />
			<basic name="hobbies" />
			<basic name="aboutMe" />
			<basic name="familyValue" />
			<basic name="familyType" />
			<basic name="familyStatus" />
			<basic name="securityCode" />
			<basic name="photoSecurityCode" />
			<basic name="lastLoggedIn">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="status" />
			<basic name="scheme" />
			<basic name="horoscope" />
			<basic name="createdBy" />
			<basic name="modifiedBy" />
			<basic name="createdDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="modifiedDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="companyId" />
			<basic name="groupId" />
			<transient name="modelAttributes" />
			<transient name="primaryKey" />
			<transient name="primaryKeyObj" />
		</attributes>
	</mapped-superclass>
	<entity class="com.vijayan.profile.model.impl.KeyImpl" name="Key">
		<table name="kalyan_key" />
		<attributes />
	</entity>
	<entity class="com.vijayan.profile.model.impl.KeyValueImpl" name="KeyValue">
		<table name="kalyan_key_value" />
		<attributes />
	</entity>
	<entity class="com.vijayan.profile.model.impl.ProfileImpl" name="Profile">
		<table name="kalyan_profile" />
		<attributes />
	</entity>
</entity-mappings>