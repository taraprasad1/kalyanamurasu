<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-lazy="false" auto-import="false">
	<import class="com.matrimony.model.Caste" />
	<import class="com.matrimony.model.City" />
	<import class="com.matrimony.model.Interaction" />
	<import class="com.matrimony.model.Key" />
	<import class="com.matrimony.model.KeyValue" />
	<import class="com.matrimony.model.Photo" />
	<import class="com.matrimony.model.Profile" />
	<import class="com.matrimony.model.ProfileKeyValue" />
	<import class="com.matrimony.model.ProfileTemp" />
	<import class="com.matrimony.model.Religion" />
	<import class="com.matrimony.model.SubCaste" />
	<class name="com.matrimony.model.impl.CasteImpl" table="matrimony_caste">
		<cache usage="read-write" />
		<id name="casteId" type="long">
			<generator class="assigned" />
		</id>
		<property name="religionId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="name" type="com.liferay.portal.dao.orm.hibernate.StringType" />
	</class>
	<class name="com.matrimony.model.impl.CityImpl" table="city">
		<cache usage="read-write" />
		<id name="cityId" type="long">
			<generator class="identity" />
		</id>
		<property name="regionId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="cityName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="status" type="com.liferay.portal.dao.orm.hibernate.BooleanType" />
	</class>
	<class name="com.matrimony.model.impl.InteractionImpl" table="matrimony_profile_interaction">
		<cache usage="read-write" />
		<id name="interactionId" type="long">
			<generator class="identity" />
		</id>
		<property name="maleId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="femaleId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="maleLastViewedFemale" type="org.hibernate.type.TimestampType" />
		<property name="maleViewedCount" type="com.liferay.portal.dao.orm.hibernate.IntegerType" />
		<property name="femaleLastViewedMale" type="org.hibernate.type.TimestampType" />
		<property name="femaleViewedCount" type="com.liferay.portal.dao.orm.hibernate.IntegerType" />
		<property name="canViewMalePhoto" type="com.liferay.portal.dao.orm.hibernate.BooleanType" />
		<property name="canViewFemalePhoto" type="com.liferay.portal.dao.orm.hibernate.BooleanType" />
		<property name="canViewMaleContact" type="com.liferay.portal.dao.orm.hibernate.BooleanType" />
		<property name="canViewFemaleContact" type="com.liferay.portal.dao.orm.hibernate.BooleanType" />
		<property name="proposedOn" type="org.hibernate.type.TimestampType" />
		<property name="proposedByMale" type="com.liferay.portal.dao.orm.hibernate.BooleanType" />
		<property name="proposalNotSeen" type="com.liferay.portal.dao.orm.hibernate.BooleanType" />
		<property name="respondedOn" type="org.hibernate.type.TimestampType" />
		<property name="responseType" type="com.liferay.portal.dao.orm.hibernate.IntegerType" />
		<property name="responseNotSeen" type="com.liferay.portal.dao.orm.hibernate.BooleanType" />
		<property name="shortlistedByMale" type="org.hibernate.type.TimestampType" />
		<property name="shortlistedByFemale" type="org.hibernate.type.TimestampType" />
		<property name="blockedByMale" type="org.hibernate.type.TimestampType" />
		<property name="blockedByFemale" type="org.hibernate.type.TimestampType" />
		<property name="capturedInMaleReport" type="com.liferay.portal.dao.orm.hibernate.IntegerType" />
		<property name="capturedInFemaleReport" type="com.liferay.portal.dao.orm.hibernate.IntegerType" />
	</class>
	<class name="com.matrimony.model.impl.KeyImpl" table="matrimony_key">
		<cache usage="read-write" />
		<id name="keyId" type="long">
			<generator class="identity" />
		</id>
		<property name="name" type="com.liferay.portal.dao.orm.hibernate.StringType" />
	</class>
	<class name="com.matrimony.model.impl.KeyValueImpl" table="matrimony_key_value">
		<cache usage="read-write" />
		<id name="valueId" type="long">
			<generator class="identity" />
		</id>
		<property name="keyId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="name" type="com.liferay.portal.dao.orm.hibernate.StringType" />
	</class>
	<class name="com.matrimony.model.impl.PhotoImpl" table="matrimony_photo">
		<cache usage="read-write" />
		<id name="photoId" type="long">
			<generator class="identity" />
		</id>
		<property name="profileId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="content" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="name" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="type" type="com.liferay.portal.dao.orm.hibernate.StringType" column="type_" />
		<property name="thumbnail" type="com.liferay.portal.dao.orm.hibernate.BooleanType" />
	</class>
	<class name="com.matrimony.model.impl.ProfileImpl" table="matrimony_profile">
		<cache usage="read-write" />
		<id name="profileId" type="long">
			<generator class="identity" />
		</id>
		<property name="profileCode" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="createdForMy" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="name" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="gender" type="com.liferay.portal.dao.orm.hibernate.BooleanType" />
		<property name="birthDateWithTime" type="org.hibernate.type.TimestampType" />
		<property name="rasi" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="star" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="dosam" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="religion" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="caste" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="subCaste" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="height" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="weight" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="complexion" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="motherTongue" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="maritalStatus" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="children" type="com.liferay.portal.dao.orm.hibernate.IntegerType" />
		<property name="country" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="state" type="com.liferay.portal.dao.orm.hibernate.StringType" column="state_" />
		<property name="city" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="address" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="pinCode" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="phone" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="mobile" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="emailAddress" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="education" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="profession" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="currency" type="com.liferay.portal.dao.orm.hibernate.StringType" column="currency_" />
		<property name="annualIncome" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="aboutMe" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="familyValue" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="familyType" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="familyStatus" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="securityCode" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="photoSecurityCode" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="lastLoggedIn" type="org.hibernate.type.TimestampType" />
		<property name="status" type="com.liferay.portal.dao.orm.hibernate.IntegerType" />
		<property name="scheme" type="com.liferay.portal.dao.orm.hibernate.IntegerType" />
		<property name="horoscope" type="com.liferay.portal.dao.orm.hibernate.BooleanType" />
		<property name="createdBy" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="modifiedBy" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="createdDate" type="org.hibernate.type.TimestampType" />
		<property name="modifiedDate" type="org.hibernate.type.TimestampType" />
		<property name="companyId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="groupId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
	</class>
	<class name="com.matrimony.model.impl.ProfileKeyValueImpl" table="matrimony_profile_key">
		<cache usage="read-write" />
		<composite-id name="primaryKey" class="com.matrimony.service.persistence.ProfileKeyValuePK">
			<key-property name="profileId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
			<key-property name="keyId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
			<key-property name="valueId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		</composite-id>
	</class>
	<class name="com.matrimony.model.impl.ProfileTempImpl" table="matrimony_profile_temp">
		<cache usage="read-write" />
		<id name="profileTempId" type="long">
			<generator class="identity" />
		</id>
		<property name="profileId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="name" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="address" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="pinCode" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="phone" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="mobile" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="emailAddress" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="aboutMe" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="scheme" type="com.liferay.portal.dao.orm.hibernate.IntegerType" />
	</class>
	<class name="com.matrimony.model.impl.ReligionImpl" table="matrimony_religion">
		<cache usage="read-write" />
		<id name="religionId" type="long">
			<generator class="assigned" />
		</id>
		<property name="name" type="com.liferay.portal.dao.orm.hibernate.StringType" />
	</class>
	<class name="com.matrimony.model.impl.SubCasteImpl" table="matrimony_subcaste">
		<cache usage="read-write" />
		<id name="subCasteId" type="long">
			<generator class="assigned" />
		</id>
		<property name="casteId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="name" type="com.liferay.portal.dao.orm.hibernate.StringType" />
	</class>
</hibernate-mapping>