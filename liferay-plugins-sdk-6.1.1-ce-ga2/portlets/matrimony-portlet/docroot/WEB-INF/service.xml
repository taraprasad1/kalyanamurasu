<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 6.0.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_6_0_0.dtd">
<service-builder package-path="com.matrimony">
	<author>matrimony</author>
	<namespace>profile</namespace>
	
	<entity name="Profile" local-service="true" remote-service="true" table="matrimony_profile">
        <!-- PK fields -->
        <column name="profileId" type="long" primary="true" id-type="identity" />
        <column name="profileCode" type="String" />
        <!-- UI fields -->
		<column name="createdForMy" type="String" />
		<column name="name" type="String" />
		<column name="gender" type="boolean" />
		<column name="birthDateWithTime" type="Date" />
		<column name="rasi" type="String" />
		<column name="star" type="String" />
		<column name="dosam" type="String" />
		<column name="religion" type="String" />
		<column name="caste" type="String" />
		<column name="subCaste" type="String" />
		<column name="height" type="String" />
		<column name="weight" type="String" />
		<column name="complexion" type="String" />
		<column name="motherTongue" type="String" />
		<column name="maritalStatus" type="String" />
		<column name="children" type="int" />
		<column name="country" type="String" />
		<column name="state" type="String" />
		<column name="city" type="String" />
		<column name="address" type="String" />
		<column name="pinCode" type="String" />
		<column name="phone" type="String" />
		<column name="mobile" type="String" />
		<column name="emailAddress" type="String" />
		<column name="education" type="String" />
		<column name="profession" type="String" />
		<column name="currency" type="String" />
		<column name="annualIncome" type="String" />
		<column name="aboutMe" type="String" />
		<column name="familyValue" type="String" />
		<column name="familyType" type="String" />
		<column name="familyStatus" type="String" />
		<column name="securityCode" type="String" />
		<column name="photoSecurityCode" type="String" />
		<column name="lastLoggedIn" type="Date" />
		<column name="status" type="int" />
		<column name="scheme" type="int" />
		<column name="horoscope" type="boolean" />
		<!-- OT fields -->
		<column name="createdBy" type="long"></column>
		<column name="modifiedBy" type="long"></column>
		<column name="createdDate" type="Date"></column>
		<column name="modifiedDate" type="Date"></column>
		<column name="companyId" type="long"></column>
		<column name="groupId" type="long"></column>

		<!-- Order -->
		<order by="desc">
			<order-column name="modifiedDate" />
		</order>
		<!-- finder methods -->
		<finder name="UserId" return-type="Collection">
			<finder-column name="createdBy" />
		</finder>
		<finder name="Status" return-type="Collection">
			<finder-column name="status"></finder-column>
		</finder>
    </entity>
    <entity name="ProfileTemp" local-service="true" remote-service="true" table="matrimony_profile_temp">
    	<column name="profileTempId" type="long" primary="true" id-type="identity" />
    	<column name="profileId" type="long" />
    	<column name="name" type="long" />
    	<column name="address" type="String" />
		<column name="pinCode" type="String" />
		<column name="phone" type="String" />
		<column name="mobile" type="String" />
		<column name="emailAddress" type="String" />
		<column name="aboutMe" type="String" />
		<column name="scheme" type="int" />
    </entity>
	<entity name="Key" local-service="true" remote-service="true" table="matrimony_key">
        <!-- PK fields -->
        <column name="keyId" type="long" primary="true" id-type="identity"></column>
        <!-- UI fields -->
        <column name="name" type="String"></column>
        
        <finder name="name" return-type="Key">
        	<finder-column name="name" />
        </finder>
    </entity>
    <entity name="KeyValue" local-service="true" remote-service="true" table="matrimony_key_value">
        <!-- PK fields -->
        <column name="valueId" type="long" primary="true" id-type="identity"></column>
        <column name="keyId" type="long"></column>
        <!-- UI fields -->
        <column name="name" type="String"></column>
        
        <finder name="KeyId" return-type="Collection">
            <finder-column name="keyId" />
        </finder>
        <finder name="valueName" return-type="KeyValue">
        	<finder-column name="name" />
        	<finder-column name="keyId" />
        </finder>
    </entity>
    <entity name="Interaction" local-service="true" table="matrimony_profile_interaction">
    	<column name="interactionId" type="long" primary="true" id-type="identity" />
    	<column name="maleId" type="long" />
    	<column name="femaleId" type="long" />
    	<column name ="maleLastViewedFemale" type="Date"/> 
		<column name="maleViewedCount" type="int" /> 
		<column name ="femaleLastViewedMale" type="Date"/> 	
		<column name="femaleViewedCount" type="int" /> 
    	<column name="canViewMalePhoto" type="boolean"/>
		<column name="canViewFemalePhoto" type="boolean"/>
		<column name="canViewMaleContact" type="boolean"/>
		<column name="canViewFemaleContact" type="boolean"/>
		<column name ="proposedOn" type="Date"/> 
		<column name="proposedByMale" type="boolean"  /> 
		<column name="proposalNotSeen" type="boolean"/>
		<column name="respondedOn" type="Date"/>
		<column name="responseType" type="int"  /> 
		<column name="responseNotSeen" type="boolean"/>
		<column name="shortlistedByMale" type="Date"/>
		<column name="shortlistedByFemale" type="Date"/>
		<column name="blockedByMale" type="Date"/>
		<column name="blockedByFemale" type="Date"/>
		<column name="capturedInMaleReport" type="int" />
		<column name="capturedInFemaleReport" type="int" />
    </entity>
    <entity name="Photo" local-service="true" table="matrimony_photo">
    	<column name="photoId" type="long" primary="true" id-type="identity" />
    	<column name="profileId" type="long" />
    	<column name="content" type="String" />
    	<column name="name" type="String" />
    	<column name="type" type="String" />
    	<column name="thumbnail" type="boolean" />
    	 <finder name="ProfileId" return-type="Collection">
            <finder-column name="profileId" />
        </finder>
    </entity>
    <entity name="Caste" table="matrimony_caste" local-service="true" remote-service="false">
    	<!-- PK fields -->                  
        <column name="casteId" type="long" primary="true" />                  
       	<column name="religionId" type="long" />                 
       	<column name="name" type="String" />
       	<finder name="ReligionId" return-type="Collection">
            <finder-column name="religionId" />
        </finder>
    </entity>
    <entity name="Religion" table="matrimony_religion" local-service="true" remote-service="false">
    	<!-- PK fields -->                  
        <column name="religionId" type="long" primary="true" />                  
       	<column name="name" type="String" />    
    </entity>
    <entity name="SubCaste" table="matrimony_subcaste" local-service="true" remote-service="false">
    	<!-- PK fields -->                  
        <column name="subCasteId" type="long" primary="true" />    
        <column name="casteId" type="long" />               
       	<column name="name" type="String" />    
       	<finder name="CasteId" return-type="Collection">
            <finder-column name="casteId" />
        </finder>
    </entity>
    <entity name="City" table="city" local-service="true" remote-service="false">  
    	<!-- PK fields -->                  
        <column name="cityId" type="long" primary="true" id-type="identity"/>                  
        	
       	<!-- Audit fields -->                  
       	
       	<column name="regionId" type="long" />                 
       	<column name="cityName" type="String" />                 
       	<column name="status" type="boolean" />                  
       	
       	<!-- Order -->                  
       	<order by="asc">                         
       		<order-column name="cityName" />                 
       	</order>                  
       	
       	<!-- Finder methods -->                  
       	<finder name="RegionId" return-type="Collection">                         
       		<finder-column name="regionId" />                 
       	</finder>                                  
       	<finder name="RegionId_City" return-type="City">                         
       		<finder-column name="regionId" />                         
       		<finder-column name="cityName" />                 
       	</finder>   
       	<finder name="Status" return-type="Collection">                         
       		<finder-column name="status" />                 
       	</finder> 
    </entity>
    <entity name="ProfileKeyValue" local-service="true" remote-service="false" table="matrimony_profile_key">
        <column name="profileId" type="long" primary="true"/>
        <column name="keyId" type="long" primary="true"/>
        <column name="valueId" type="long" primary="true"/>
        <finder name="ProfileAndKeyId" return-type="Collection">
        	<finder-column name="keyId" />
        	<finder-column name="profileId" />
        </finder>
        <finder name="KeyandValueId" return-type="Collection">
        	<finder-column name="keyId" />
        	<finder-column name="valueId" />
        </finder>
    </entity>
    <exceptions>
    	<exception>InvalidImageType</exception>
		<exception>ImageSizeExceedsLimit</exception>
		<exception>InvalidImage</exception>
    </exceptions>
</service-builder>