<aui:select name="createdForMy" label="profile-created-for" inlineLabel="true" help="profile-help-created-for-me">
	<% for(KeyValue values : ProfileUtil.getKeyValueList("createdFor")){ %>
		<aui:option value="<%= values.getName() %>"><%= values.getName() %></aui:option>
	<% } %>
</aui:select>
<aui:input type="text" name="name" label="profile-name" inlineLabel="true"/>
<aui:input type="radio" name="gender" label="profile-male" inlineLabel="right" inlineField="true" checked="<%=true %>" value="male"/>
<aui:input type="radio" name="gender" inlineLabel="right"  label="profile-female"  value="female"/>