<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 6.0.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_6_0_0.dtd">
<service-builder package-path="com.vijayan.profile">
	<author>vijayan</author>
	<namespace>profile</namespace>
	
	<entity name="Profile" local-service="true" remote-service="true" table="kalyan_profile">
        <!-- PK fields -->
        <column name="profileId" type="long" primary="true" id-type="identity" />
        <column name="profileCode" type="long" />
        <!-- UI fields -->
		<column name="createdForMy" type="String" />
		<column name="name" type="String" />
		<column name="gender" type="boolean" />
		<column name="birthDateWithTime" type="Date" />
		<column name="rasi" type="String" />
		<column name="star" type="String" />
		<column name="dosam" type="String" />
		<column name="religion" type="String" />
		<column name="caste" type="String" />
		<column name="subCaste" type="String" />
		<column name="height" type="String" />
		<column name="weight" type="String" />
		<column name="complexion" type="String" />
		<column name="motherTongue" type="String" />
		<column name="languageKnown" type="String" />
		<column name="maritalStatus" type="String" />
		<column name="children" type="int" />
		<column name="country" type="String" />
		<column name="state" type="String" />
		<column name="city" type="String" />
		<column name="address" type="String" />
		<column name="pinCode" type="String" />
		<column name="phone" type="String" />
		<column name="mobile" type="String" />
		<column name="emailAddress" type="String" />
		<column name="education" type="String" />
		<column name="profession" type="String" />
		<column name="currency" type="String" />
		<column name="annualIncome" type="String" />
		<column name="hobbies" type="String" />
		<column name="aboutMe" type="String" />
		<column name="familyValue" type="String" />
		<column name="familyType" type="String" />
		<column name="familyStatus" type="String" />
		<column name="securityCode" type="String" />
		<column name="photoSecurityCode" type="String" />
		<column name="lastLoggedIn" type="Date" />
		<column name="status" type="int" />
		<column name="scheme" type="int" />
		<column name="horoscope" type="boolean" />
		<!-- OT fields -->
		<column name="createdBy" type="long"></column>
		<column name="modifiedBy" type="long"></column>
		<column name="createdDate" type="Date"></column>
		<column name="modifiedDate" type="Date"></column>
		<column name="companyId" type="long"></column>
		<column name="groupId" type="long"></column>

		<!-- Order -->
		<order by="desc">
			<order-column name="modifiedDate" />
		</order>
		<!-- finder methods -->
		<finder name="UserId" return-type="Collection">
			<finder-column name="createdBy" />
		</finder>
		<finder name="Status" return-type="Collection">
			<finder-column name="status"></finder-column>
		</finder>
    </entity>
	<entity name="Key" local-service="true" remote-service="true" table="kalyan_key">
        <!-- PK fields -->
        <column name="keyId" type="long" primary="true" id-type="identity"></column>
        <!-- UI fields -->
        <column name="name" type="String"></column>
        
        <finder name="name" return-type="Key">
        	<finder-column name="name" />
        </finder>
    </entity>
    <entity name="KeyValue" local-service="true" remote-service="true" table="kalyan_key_value">
        <!-- PK fields -->
        <column name="valueId" type="long" primary="true" id-type="identity"></column>
        <column name="keyId" type="long"></column>
        <!-- UI fields -->
        <column name="name" type="String"></column>
        
        <finder name="KeyId" return-type="Collection">
            <finder-column name="keyId" />
        </finder>
        <finder name="valueName" return-type="KeyValue">
        	<finder-column name="name" />
        	<finder-column name="keyId" />
        </finder>
    </entity>
</service-builder>